on/scroll/Scroll\"\n\n  export default {\n    name: \"Detail\",\n    components: {\n      \n      DetailNavBar,\n      DetailSwiper,\n      DetailBaseInfo,\n      DetailShopInfo,\n      getGoodsDetail,\n      Goods,\n\n      Shop\n    },\n    data() {\n      return {\n        iid: null,\n        topImages: [],\n        goods: null,\n        shop: null,\n      }\n    },\n    created() {\n      this.iid = this.$route.params.iid;\n      getGoodsDetail(this.iid).then(res => {\n        const data = res.result;\n        console.log(data);\n        this.topImages = data.itemInfo.topImages;\n\n        //商品信息\n        this.goods = new Goods(data.itemInfo, data.columns, data.shopInfo.services);\n        console.log(this.goods);\n\n        //店铺信息\n        this.shop = new Shop(data.shopInfo);\n      }).catch(err => {\n        console.log(\"数据请求失败\");\n      })\n    }\n\n  }\n</script>\n\n<style scoped>\n\n  .detail {\n    height: 100vh;\n    background-color: #fff;\n    position: relative;\n    z-index: 1;\n  }\n  .content {\n    background-color: #fff;\n    height: calc(100% - 44px);\n  }\n</style>\n"]}]}