AAUA,GA7BA;AA8BA,EAAA,OA9BA,qBA8BA;AAAA;;AAEA,SAAA,GAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,GAAA;AACA,IAAA,cAAA,CAAA,KAAA,GAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,IAAA,GAAA,GAAA,CAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,EAFA,CAIA;;AACA,MAAA,KAAA,CAAA,SAAA,GAAA,IAAA,CAAA,QAAA,CAAA,SAAA,CALA,CAOA;;AACA,MAAA,KAAA,CAAA,KAAA,GAAA,IAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA,IAAA,CAAA,OAAA,EAAA,IAAA,CAAA,QAAA,CAAA,QAAA,CAAA,CARA,CASA;AAEA;;AACA,MAAA,KAAA,CAAA,IAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA,QAAA,CAAA,CAZA,CAcA;;AACA,MAAA,KAAA,CAAA,UAAA,GAAA,IAAA,CAAA,UAAA,CAfA,CAiBA;;AACA,MAAA,KAAA,CAAA,UAAA,GAAA,IAAA,UAAA,CAAA,IAAA,CAAA,UAAA,CAAA,IAAA,EAAA,IAAA,CAAA,UAAA,CAAA,IAAA,CAAA,CAlBA,CAoBA;;AACA,UAAA,IAAA,CAAA,IAAA,CAAA,KAAA,GAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AACA;AACA,KAxBA,EAwBA,KAxBA,CAwBA,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,KA1BA,EAHA,CAgCA;;AACA,IAAA,YAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,MAAA,KAAA,CAAA,aAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAFA;AAGA,GAlEA;AAmEA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,OAAA;AACA;AAHA;AAnEA,CAAA","sourcesContent":["<template>\n  <div class=\"detail\">\n    <detail-nav-bar class=\"detail-nav\"/>\n    <scroll class=\"content\" ref=\"scroll\">\n      <detail-swiper :top-images=\"topImages\"></detail-swiper>\n      <detail-base-info  v-if=\"goods\" :goods=\"goods\" ></detail-base-info>\n      <detail-shop-info v-if=\"shop\" :shop=\"shop\"></detail-shop-info>\n      <detail-goods-info v-if=\"detailInfo\" :detail-info=\"detailInfo\" @imageLoad=\"imageLoad\"></detail-goods-info>\n      <detail-param-info v-if=\"goodsParam\" :param-info=\"goodsParam\" />\n      <detail-comment-info v-if=\"commentInfo\" :comment-info=\"commentInfo\"/>\n      <detail-recommend v-if=\"recommendInfo\" :recommend-info=\"recommendInfo\"/>\n    </scroll>\n  </div>\n</template>\n\n<script>\n  import DetailNavBar from './childComps/DetailNavBar.vue';\n  import DetailSwiper from './childComps/DetailSwiper.vue';\n  import DetailBaseInfo from \"./childComps/DetailBaseInfo\";\n  import DetailShopInfo from \"./childComps/DetailShopInfo\";\n  import DetailGoodsInfo from \"./childComps/DetailGoodsInfo\";\n  import DetailParamInfo from \"./childComps/DetailParamInfo\";\n  import DetailRecommend from \"./childComps/DetailRecommend\";\n  import DetailCommentInfo from \"./childComps/DetailCommentInfo\"\n\n  import {getGoodsDetail, Goods, Shop, GoodsParam, getRecommend} from 'network/detail.js'\n  import Scroll from \"components/common/scroll/Scroll\"\n\n  export default {\n    name: \"Detail\",\n    components: {\n      Scroll,\n      DetailNavBar,\n      DetailSwiper,\n      DetailBaseInfo,\n      DetailShopInfo,\n      DetailGoodsInfo,\n      DetailParamInfo,\n      DetailRecommend,\n      getGoodsDetail,\n      DetailCommentInfo,\n      Goods,\n      Shop,\n      GoodsParam,\n      getRecommend\n    },\n    data() {\n      return {\n        iid: null,\n        topImages: [],\n        goods: null,\n        shop: null,\n        detailInfo: null,\n        goodsParam: null,\n        commentInfo: {},\n        recommendInfo:[]\n      }\n    },\n    created() {\n      \n      this.iid = this.$route.params.iid;\n      getGoodsDetail(this.iid).then(res => {\n        const data = res.result;\n        console.log(data);\n\n        //1.顶部轮播图\n        this.topImages = data.itemInfo.topImages;\n\n        //2.商品信息\n        this.goods = new Goods(data.itemInfo, data.columns, data.shopInfo.services);\n        // console.log(this.goods);\n\n        //3.店铺信息\n        this.shop = new Shop(data.shopInfo);\n\n        //4.描述信息\n        this.detailInfo = data.detailInfo;\n\n        //5.参数\n        this.goodsParam = new GoodsParam(data.itemParams.info, data.itemParams.rule);\n\n        //6.评论\n        if(data.rate.cRate>0){\n          this.commentInfo = data.rate.list[0]\n        }\n      }).catch(err => {\n        console.log(\"数据请求失败\");\n      })\n\n              \n        //7.推荐数据\n      getRecommend().then(res => {\n        this.recommendInfo = res.data.list;\n      })\n    },\n    methods:{\n      imageLoad(){\n        this.$refs.scroll.refresh();\n      }\n    }\n  }\n</script>\n\n<style scoped>\n\n  .detail {\n    position: relative;\n    z-index: 9;\n    background-color: #fff;\n    height: 100vh;\n  }\n  .content {\n    background-color: #fff;\n    height: calc(100% - 44px);\n  }\n\n  .detail-nav {\n    position: relative;\n    z-index: 9;\n    background-color: #fff;\n  }\n\n</style>\n"],"sourceRoot":"src/views/detail"}]}